{
  "name": "unplugin-img-compress",
  "description": "ðŸ”¥ Image compression plugin based on tinypng",
  "private": false,
  "type": "module",
  "version": "1.1.0",
  "packageManager": "pnpm@6.32.4",
  "keywords": [
    "webpack",
    "rollup",
    "compression-library",
    "vite",
    "esbuild",
    "unplugin",
    "compression-img"
  ],
  "license": "MIT",
  "homepage": "https://github.com/baiwusanyu-c/unplugin-img-compress#README",
  "author": "baiwusanyu-c",
  "repository": "https://github.com/baiwusanyu-c/unplugin-img-compress",
  "bugs": "https://github.com/baiwusanyu-c/unplugin-img-compress/issues",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "import": "./index.js"
    }
  },
  "files": [
    "bin",
    "core",
    "utils",
    "package.json",
    "README.md",
    "README.ZH-CN.md",
    "index.js",
    "index.cjs",
    "index.d.ts"
  ],
  "bin": {
    "unp-img": "./bin/index.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./index.d.ts"
      ]
    }
  },
  "scripts": {
    "init": "pnpm i",
    "lint:fix": "eslint --fix ./ --ext .vue,.js,.ts,.jsx,.tsx,.json ",
    "dev": "pnpm run clean && pnpm run --filter @unplugin-img-compress/build dev",
    "play:runtime-build": "pnpm run --filter @unplugin-img-compress/play runtime:build",
    "play:runtime-dev": "pnpm run --filter @unplugin-img-compress/play runtime:dev",
    "build": "pnpm run clean && pnpm run --filter @unplugin-img-compress/build build",
    "release": "bumpp package.json --commit --push --tag",
    "clean": "rimraf dist",
    "prepare": "npx simple-git-hooks",
    "test": "vitest",
    "test:update": "vitest -u",
    "test:coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "chalk": "4.1.2",
    "chokidar": "^3.5.3",
    "fs-extra": "^10.0.1",
    "ora": "^6.1.2",
    "tinify": "^1.7.1",
    "unconfig": "^0.3.5"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "chokidar": "^3.5.3",
    "fs-extra": "^10.0.1",
    "ora": "^6.1.2",
    "tinify": "^1.7.1",
    "unconfig": "^0.3.5",
    "unplugin": "^0.9.6"
  },
  "devDependencies": {
    "@baiwusanyu/eslint-config": "^1.0.10",
    "@types/gulp": "^4.0.10",
    "@unplugin-img-compress/build": "workspace:*",
    "@unplugin-img-compress/core": "workspace:*",
    "@unplugin-img-compress/entry": "workspace:*",
    "@unplugin-img-compress/play": "workspace:*",
    "@unplugin-img-compress/utils": "workspace:*",
    "@vitest/coverage-c8": "^0.29.8",
    "@vitest/ui": "0.30.1",
    "bumpp": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "esno": "^0.16.3",
    "gulp": "^4.0.2",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.1.1",
    "rimraf": "^4.4.1",
    "rollup": "^2.79.0",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.5.0",
    "typescript": "^5.0.3",
    "vite": "^4.0.4",
    "vitest": "0.29.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node vertify-commit.js"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": [
      "eslint ./ --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@baiwusanyu"
    ]
  }
}
